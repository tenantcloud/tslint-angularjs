{
	"compilerOptions": {
		"module": "commonjs",
		"resolveJsonModule": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"strict": true,
		"importHelpers": true,
		"declaration": true,
		"sourceMap": false
	},
	"exclude": ["node_modules", "**/*.spec.ts"]
}

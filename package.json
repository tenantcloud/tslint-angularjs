{
	"name": "tslint-angularjs",
	"version": "2.0.0",
	"description": "Custom rules for angularjs code with angular-ts-decorators",
	"repository": "https://github.com/tenantcloud/tslint-angularjs",
	"main": "tslint.json",
	"scripts": {
		"start": "rm -rf lib && tsc -w --project src",
		"build": "rm -rf lib && tsc --project src",
		"watch": "rm -rf lib && tsc -w --project src",
		"test": "npm run build && jest"
	},
	"keywords": [
		"tslint",
		"angularjs",
		"angular-ts-decorators"
	],
	"author": "Misha Hrokhola",
	"license": "ISC",
	"dependencies": {
		"tslint": "^5.16.0",
		"tsutils": "^3.21.0",
		"typescript": ">=4.0.0"
	},
	"devDependencies": {
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.10",
		"jest": "^27.3.1",
		"prettier": "^2.4.1",
		"ts-jest": "^27.0.7",
		"tslint-config-prettier": "^1.18.0"
	}
}
